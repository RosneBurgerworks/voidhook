<Tab name="Collective" padding="4 4 4 4">
    <Box height="content" width="content" padding="12 6 6 6" name="Collective">
        <List width="200">
            <AutoVariable width="fill" target="misc.anti-afk" label="Enable antiafk"/>
            <AutoVariable width="fill" target="misc.auto-abandon" label="Enable auto abandon"/>
            <AutoVariable width="fill" target="misc.auto-balance-spam" label="Enable auto balance spam"/>
            <AutoVariable width="fill" target="misc.die-if-vac" label="Crash on VAC"/>
            <AutoVariable width="fill" target="misc.disconnect-reason" label="Disconnect reason"/>
            <AutoVariable width="fill" target="misc.engine-prediction" label="Engine prediction"/>
            <AutoVariable width="fill" target="misc.flashlight-spam" label="Flashlight spam" tooltip="Used in Half Life Death Match."/>
            <AutoVariable width="fill" target="misc.force-halloween" label="Force halloween"/>
            <AutoVariable width="fill" target="misc.no-lerp" label="Nolerp"/>
            <AutoVariable width="fill" target="misc.full-auto" label="Fully automated firing"/>
            <AutoVariable width="fill" target="misc.show-spectators" label="Show spectators"/>
            <AutoVariable width="fill" target="misc.tauntslide" label="Tauntslide"/>
            <AutoVariable width="fill" target="misc.sv-cheats-bypass" label="sv_cheats Bypass" tooltip="Allows you to use some sv_cheats commands (clientside)"/>
            <AutoVariable width="fill" target="announcer" label="Enable announcer"/>
        </List>
    </Box>
    <Box height="content" width="content" padding="12 6 6 6" name="Steam RPC" y="250">
        <List width="200">
            <AutoVariable width="fill" target="misc.rich-presence.enable" label="Steam RPC" tooltip="Enable Steam Rich Presence"/>
            <LabeledObject width="fill" label="Match group" tooltip="Which match group should be used?">
                <Select target="misc.rich-presence.group">
                    <Option name="Special Event" value="0"/>
                    <Option name="MvM Mann Up" value="1"/>
                    <Option name="Competitive" value="2"/>
                    <Option name="Casual" value="3"/>
                    <Option name="MvM Boot Camp" value="4"/>
                </Select>
            </LabeledObject>
            <AutoVariable width="fill" target="misc.rich-presence.menu-override" label="Override in menu" tooltip="Override match group to 'Main Menu' when in main menu"/>
            <LabeledObject width="fill" label="Map text" tooltip="Which map text should be used?">
                <Select target="misc.rich-presence.map">
                    <Option name="Custom" value="0"/>
                    <Option name="Fedoraware" value="1"/>
                    <Option name="Figoraware" value="2"/>
                    <Option name="Meowhook.club" value="3"/>
                    <Option name="Rathook.cc" value="4"/>
                    <Option name="Nitro.tf" value="5"/>
                    <Option name="Voidhook" value="6"/>
                    <Option name="Pastedhook" value="7"/>
                    <Option name="Fuckhook" value="8"/>
                    <Option name="Burgerhook" value="9"/>
                    <Option name="Cathook.club" value="10"/>
                </Select>
            </LabeledObject>
            <AutoVariable width="fill" target="misc.rich-presence.map-text" label="Custom map text" tooltip="Sets custom map text when 'Custom' is selected in 'Map text'"/>
            <AutoVariable width="fill" target="misc.rich-presence.party-size" label="Group size" tooltip="Sets party size"/>
        </List>
    </Box>
    <Box height="content" width="content" padding="12 6 6 6" name="Fake name" x="215">
        <List width="200">
            <AutoVariable width="fill" target="name.custom" label="Custom name"/>
            <AutoVariable width="fill" target="name.custom.ipc" label="IPC name"/>
            <LabeledObject width="fill" label="Namesteal">
                <Select target="name.namesteal">
                    <Option name="Disabled" value="0"/>
                    <Option name="Passive" value="1"/>
                    <Option name="Active" value="2"/>
                </Select>
            </LabeledObject>
            <AutoVariable width="fill" target="name.namesteal.reconnect" label="Namesteal reconnect"/>
            <AutoVariable width="fill" target="name.namesteal.use-newlines" label="Namesteal newlines" tooltip="Use glitchy newlines, will make your name invisible."/>
            <AutoVariable width="fill" target="misc.random-name" label="Random name"/>
        </List>
    </Box>
    <Box height="content" width="content" padding="12 6 6 6" name="Extra" x="215" y="115">
        <List width="200">
            <AutoVariable width="fill" target="noisemaker-spam.enable" label="Noisemaker spam"/>
            <AutoVariable width="fill" target="player-tools.betrayal-limit" label="Betrayal limit"/>
            <AutoVariable width="fill" target="player-tools.ignore.hoovy" label="Ignore hoovies"/>
            <AutoVariable width="fill" target="player-tools.ignore.taunting" label="Ignore taunting players"/>
            <AutoVariable width="fill" target="player-tools.auto-party" label="Automatically add party state"/>
            <AutoVariable width="fill" target="killstreak.enable" label="Enable killstreak"/>
            <AutoVariable width="fill" target="misc.backpack-expander.enabled" label="Expand backpack"/>
            <AutoVariable width="fill" target="misc.backpack-expander.slot-count" label="Backpack slots" min="1" max="3000"/>
            <AutoVariable width="fill" target="mvmtools.instant-revive" label="MVM Instant revive"/>
            <AutoVariable width="fill" target="mvmtools.auto-f4" label="Auto F4"/>
        </List>
    </Box>
</Tab>
